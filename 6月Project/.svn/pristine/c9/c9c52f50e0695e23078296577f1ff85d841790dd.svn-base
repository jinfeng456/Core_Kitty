using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace GK.WCS.Common.dto {
    [Serializable]
    public class TaskStat {
        public long workAllTime//启动后所有执行时间
        {
            get; set;
        }
        public long worklastTime//最近一次执行时间
        {
            get; set;
        }
        public long maxTime//最大执行时间
        {
            get; set;
        }
        public long minTime//最小执行时间
        {
            get; set;
        }
        public long workCount//执行次数
        {
            get; set;
        }
        public String className
        {
            get; set;
        }
        public int time  {//执行间隔
            get; set;
        }
        public int exceptionCount//异常数量
        {
            get; set;
        }
    }
}
