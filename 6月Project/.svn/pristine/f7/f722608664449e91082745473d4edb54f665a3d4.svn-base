
      
        

using System;      
using System.Collections.Generic;
using System.Collections;
using YabaoSoft.Common.Orm;
using Huax_WMS.Sc.Entity;

namespace Huax_WMS.Sc.DAO
{
    public class sys_UsersReador 
    {

    
        public DataSelector<sys_UsersField> Selector { get; set; }
    
        private YabaoSoft.Common.PagerAdapter _PagerAdapter;
        /// <summary>
        /// 分页适配器,默认每页1000条
        /// </summary>
        public YabaoSoft.Common.PagerAdapter PagerAdapter
        {
            set { _PagerAdapter = value; }
            get
            {
                if (_PagerAdapter == null)
                {
                    _PagerAdapter = new YabaoSoft.Common.PagerAdapter();
                    _PagerAdapter.RecPerPage = 1000;
                }
                return _PagerAdapter;
            }
        }

        private List<Huax_WMS.Sc.DBL.sys_UsersSelectOrder> _SelectOrders;
        /// <summary>
        /// 列表排序
        /// </summary>
        public List<Huax_WMS.Sc.DBL.sys_UsersSelectOrder> SelectOrders
        {
            set { _SelectOrders = value; }
            get {
                if (_SelectOrders == null)
                    _SelectOrders = new List<Huax_WMS.Sc.DBL.sys_UsersSelectOrder>();        
                    return _SelectOrders; }
        }




        /// <summary>
        /// 执行读取
        /// </summary>
        /// <returns></returns>
        public List<Huax_WMS.Sc.Entity.sys_Users> Read()
        {
            if (_PagerAdapter == null)
            {
                _PagerAdapter = new YabaoSoft.Common.PagerAdapter();
                _PagerAdapter.RecPerPage = 1000;
            }
            return Huax_WMS.Sc.DBL.sys_Users.GetList(this.Selector, _PagerAdapter, _SelectOrders);
        }


        #region 新增的静态方法

        public static List<Huax_WMS.Sc.Entity.sys_Users> ReadAll()
        {
            sys_UsersReador reador = new sys_UsersReador();
            return reador.Read();
        }

        public static Huax_WMS.Sc.Entity.sys_Users Read(int Id)
        {
            sys_UsersReador reador = new sys_UsersReador();
            reador.Selector = Huax_WMS.Sc.Entity.sys_Users.Prop.Id == Id;
            List<Huax_WMS.Sc.Entity.sys_Users> rtli = reador.Read();
            if (rtli.Count == 1)
                return rtli[0];
            return null;
        }
        
        public static Huax_WMS.Sc.Entity.sys_Users ReadOne(DataSelector<sys_UsersField> Selector
         )
        {
            List<Huax_WMS.Sc.Entity.sys_Users> rtli = Read(Selector);
            if (rtli.Count > 0 )
                return rtli[0];
            return null;
        }

        public static List<Huax_WMS.Sc.Entity.sys_Users> Read(int[] ids)
        {
            if (ids.Length == 0)
                return new List<sys_Users>();
                
            sys_UsersReador reador = new sys_UsersReador();
            foreach (var item in ids)
            {
                reador.Selector /= Huax_WMS.Sc.Entity.sys_Users.Prop.Id == item;
            }
            return reador.Read();
        }

        public static List<Huax_WMS.Sc.Entity.sys_Users> Read(DataSelector<sys_UsersField> Selector
            , YabaoSoft.Common.PagerAdapter PagerAdapter
            , List<Huax_WMS.Sc.DBL.sys_UsersSelectOrder> SelectOrders
            )
        {
            sys_UsersReador reador = new sys_UsersReador();
            if (Selector != null) reador.Selector = Selector;
            if (PagerAdapter != null) reador.PagerAdapter = PagerAdapter;
            if (SelectOrders != null) reador.SelectOrders = SelectOrders;
            return reador.Read();
        }

        public static List<Huax_WMS.Sc.Entity.sys_Users> Read(DataSelector<sys_UsersField> Selector
           , YabaoSoft.Common.PagerAdapter PagerAdapter
           )
        {
            return sys_UsersReador.Read(Selector, PagerAdapter, null);
        }

        public static List<Huax_WMS.Sc.Entity.sys_Users> Read(DataSelector<sys_UsersField> Selector
         )
        {
            return sys_UsersReador.Read(Selector, null, null);
        }
        #endregion
        
        
        #region 读取关联对象
        /// <summary>
        /// 读取关联对象
        /// </summary>
        /// <param name="ent">本体对象</param>
        /// <param name="props">需要读取的关联字段，多个用“,”</param>
        public static void ReadRela(sys_Users ent, params sys_Users_RelaProp[] props)
        {
            List<sys_Users_RelaProp> propLi = new List<sys_Users_RelaProp>(props);

        }

        /// <summary>
        /// 读取对象列表的关联对象
        /// </summary>
        /// <param name="ents">本体对象列表</param>
        /// <param name="props">需要读取的关联字段，多个用“,”</param>
        public static void ReadRela(List<sys_Users> ents, params sys_Users_RelaProp[] props)
        {
            List<sys_Users_RelaProp> propLi = new List<sys_Users_RelaProp>(props);

        }

        #endregion

    }
}

        
