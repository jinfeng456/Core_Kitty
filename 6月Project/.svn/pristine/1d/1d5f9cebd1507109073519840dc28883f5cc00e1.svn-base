using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using CMNetLib.Robots.CarrierChain;

namespace GK.WCS.Common.core.dto {
   public class HySignalState :SignalState {
        public bool isIdle() {
            if(TaskNo > 0) {
                return false;
            }
            return !OnOff;
        }


        public bool isTask(int task) {
            
            return TaskNo== task;
        }
        public bool needTask() {
            if(TaskNo > 0) {
                return false;
            }
            return OnOff;
        }


    }
}
