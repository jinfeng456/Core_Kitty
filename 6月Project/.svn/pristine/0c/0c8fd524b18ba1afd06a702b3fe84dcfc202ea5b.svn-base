import axios from '@/http/axios'

// 分页查询
export const findPage = (data) => {
    return axios({
        url: '/statMonth/findPage',
        method: 'post',
        data
    })
}

export const getAllList = () => {
    return axios({
        url: '/statMonth/getAllList',
        method: 'post'
    })
}

	