import axios from '../../axios'

/* 
 * 任务查询
 */

// 分页查询
export const findPage = (data) => {
    return axios({
        url: '/coreTask/findPage',
        method: 'post',
        data
    })
}

export const GetListByTaskId = (data) => {
    return axios({
        url: '/coreTask/GetListByTaskId',
        method: 'post',
        data
    })
}