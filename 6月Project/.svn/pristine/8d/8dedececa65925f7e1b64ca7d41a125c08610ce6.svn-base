import axios from '../../axios'

export const getDetials = (data) => {
    return axios({
        url: '/whSoIn/getDetials',
        method: 'post',
        data
    })
}

export const saveWhSoIn = (data) => {
  return axios({
    url: '/whSoIn/saveWhSoIn',
    method: 'post',
    data
  })
}

