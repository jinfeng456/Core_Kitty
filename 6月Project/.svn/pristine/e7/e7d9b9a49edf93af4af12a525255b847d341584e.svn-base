using GK.Common.Entity;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace GK.WWS.Entity
{
    [Serializable]
    public class PhysicalLocation : BaseEntity
    {
      

     
        /// <summary>
        /// 货位的X轴
        /// </summary>
        public int X
        {
            get; set;
        }

        /// <summary>
        /// 货位的Y
        /// </summary>
        public int Y
        {
            get; set;
        }
        public int Z
        {
            get; set;
        }

        /// <summary>
        /// 第几行
        /// </summary>
        public int RowNo
        {
            get; set;
        }
        /// <summary>
        /// 第几列
        /// </summary>
        public int ColNo
        {
            get; set;
        }
        /// <summary>
        /// 货架方向
        /// </summary>
        public int Direction
        {
            get; set;
        }


        public int signPoint
        {
            get; set;
        }

        /// <summary>
        /// 所述货架
        /// </summary>
        public int ShelfId
        {
            get; set;
        }

        public int deep
        {
            get; set;
        }



    }
}
