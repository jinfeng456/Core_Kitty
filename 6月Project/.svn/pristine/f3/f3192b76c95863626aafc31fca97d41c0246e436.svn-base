        private List<Huax_WMS.Sc.DBL.sys_RolesSelectOrder> _SelectOrders;
        /// <summary>
        /// 列表排序
        /// </summary>
        public List<Huax_WMS.Sc.DBL.sys_RolesSelectOrder> SelectOrders
        {
            set { _SelectOrders = value; }
            get {
                if (_SelectOrders == null)
                    _SelectOrders = new List<Huax_WMS.Sc.DBL.sys_RolesSelectOrder>();        
                    return _SelectOrders; }
        }




        /// <summary>
        /// 执行读取
        /// </summary>
        /// <returns></returns>
        public List<Huax_WMS.Sc.Entity.sys_Roles> Read()
        {
            if (_PagerAdapter == null)
            {
                _PagerAdapter = new YabaoSoft.Common.PagerAdapter();
                _PagerAdapter.RecPerPage = 1000;
            }
            return Huax_WMS.Sc.DBL.sys_Roles.GetList(this.Selector, _PagerAdapter, _SelectOrders);
        }


        #region 新增的静态方法

        public static List<Huax_WMS.Sc.Entity.sys_Roles> ReadAll()
        {
            sys_RolesReador reador = new sys_RolesReador();
            return reador.Read();
        }

        public static Huax_WMS.Sc.Entity.sys_Roles Read(int Id)
        {
            sys_RolesReador reador = new sys_RolesReador();
            reador.Selector = Huax_WMS.Sc.Entity.sys_Roles.Prop.Id == Id;
            List<Huax_WMS.Sc.Entity.sys_Roles> rtli = reador.Read();
            if (rtli.Count == 1)
                return rtli[0];
            return null;
        }
        
        public static Huax_WMS.Sc.Entity.sys_Roles ReadOne(DataSelector<sys_RolesField> Selector
         )
        {
            List<Huax_WMS.Sc.Entity.sys_Roles> rtli = Read(Selector);
            if (rtli.Count > 0 )
                return rtli[0];
            return null;
        }

        public static List<Huax_WMS.Sc.Entity.sys_Roles> Read(int[] ids)
        {
            if (ids.Length == 0)
                return new List<sys_Roles>();
                
            sys_RolesReador reador = new sys_RolesReador();
            foreach (var item in ids)
            {
                reador.Selector /= Huax_WMS.Sc.Entity.sys_Roles.Prop.Id == item;
            }
            return reador.Read();
        }

        public static List<Huax_WMS.Sc.Entity.sys_Roles> Read(DataSelector<sys_RolesField> Selector
            , YabaoSoft.Common.PagerAdapter PagerAdapter
            , List<Huax_WMS.Sc.DBL.sys_RolesSelectOrder> SelectOrders
            )
        {
            sys_RolesReador reador = new sys_RolesReador();
            if (Selector != null) reador.Selector = Selector;
            if (PagerAdapter != null) reador.PagerAdapter = PagerAdapter;
            if (SelectOrders != null) reador.SelectOrders = SelectOrders;
            return reador.Read();
        }

        public static List<Huax_WMS.Sc.Entity.sys_Roles> Read(DataSelector<sys_RolesField> Selector
           , YabaoSoft.Common.PagerAdapter PagerAdapter
           )
        {
            return sys_RolesReador.Read(Selector, PagerAdapter, null);
        }

        public static List<Huax_WMS.Sc.Entity.sys_Roles> Read(DataSelector<sys_RolesField> Selector
         )
        {
            return sys_RolesReador.Read(Selector, null, null);
        }
