using GK.WCS.Common;
using GK.WCS.Crane.enumerate;
using System;
using System.Collections.Generic;
using System.Linq;
using System.Runtime.Serialization.Formatters;
using System.Text;

namespace GK.WCS.Crane
{
    
    public abstract class GkCraneStatusBase
    {        
        bool fault= true;
        public bool isfault() 
        { 
            return fault;
        }
        public void setFault() 
        {
            fault= true;
        }
        public void clearFault()
        {
            fault = false;
        }
        protected long lastTime;
        public  abstract bool checkCrane();

        public abstract List<String> getError();
        public abstract int getX();
        public abstract int getY();
        public abstract int getZ();

        public abstract  void parseError(Byte[] b);

        public abstract void parseStatus(Byte[] b);

        public  long getLastTime()
        {
            return lastTime;
        }
    }
}
